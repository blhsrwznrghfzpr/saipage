{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "typescript-eslint-parser"
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:vue/recommended",
    "typescript",
    "prettier"
  ],
  "rules": {
    "no-trailing-spaces": "error",
    "no-console": "off",
    "typescript/explicit-member-accessibility": "none"
  }
}
